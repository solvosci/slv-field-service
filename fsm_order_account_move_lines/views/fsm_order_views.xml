<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="fsm_order_tree_views" model="ir.ui.view">
        <field name="name">fsm.order.form (in FSM Order Account move Line)</field>
        <field name="model">fsm.order</field>
        <field name="inherit_id" ref="fieldservice.fsm_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='equipements_page']" position="after">
                <group  name="invoice_expenses" string="Invoice">
                    <field name="account_move_line_ids" nolabel="1" >
                        <tree create="0" >
                            <field name="move_id" />
                            <field name="date" />
                            <field name="product_id" />
                            <field name="name" />
                            <field name="debit" />
                            <field name="credit" />
                        </tree>
                    </field>
                </group>
            </xpath> 
        </field>
    </record>
</odoo>
